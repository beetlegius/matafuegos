<div class="page-header">
  <h1><%= Sector.model_name.human.pluralize(I18n.locale) %></h1>
</div>

<div class="row">
  <div class="col-md-12">

    <table id="sectors" class="table table-condensed">
      <thead>
        <tr>
          <th class="text-right"><%= Sector.human_attribute_name(:id) %></th>
          <th><%= Sector.human_attribute_name(:name) %></th>
          <th class="text-right"><%= Sector.human_attribute_name(:latitude) %></th>
          <th class="text-right"><%= Sector.human_attribute_name(:longitude) %></th>
          <th></th>
        </tr>
      </thead>
      <%= content_tag :tbody, render(@sectors) %>
    </table>

  </div>
</div>
